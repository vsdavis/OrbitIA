{

  "type": "commonjs",
  "name": "orbitia",
  "displayName": "orbitIA",
  "description": "VS Code extension using AI",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:orbitia.openChat"
      ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "orbitia.openChat",
        "title": "Open OrbitIA Chatbot"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  },
  "dependencies": {

    "node-fetch": "^3.0.0"

    "@google/generative-ai": "^0.21.0",
    "node-fetch": "^3.3.2"
  }
}







